---
import Heading from "@Component/Readme/Heading.astro";
import Row from "@Component/Readme/Row.astro";
import Table from "@Component/Readme/Table.astro";

const { Open } = Astro.props;
---

<>
	<Table {Open}>
		<Heading
			DarkIcon="HTTPS://PlayForm.LTD/Image/GitHub/Biome.svg"
			LightIcon="HTTPS://PlayForm.LTD/Image/GitHub/Biome.svg"
			Text="Biome"
		/>

		<Row
			Package={new Set([
				"GitHub:CodeEditorLand/DependencyBiome",
				"GitHub:CodeEditorLand/DependencyBiomeCargo",
				"GitHub:CodeEditorLand/DependencyBiomeNPM",
				"GitHub:CodeEditorLand/DependencyBiomeService",
			])}
		/>
	</Table>
</>
